<Window x:Class="Data2Check.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Data2Check"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ProgressBar x:Name="progressbar" HorizontalAlignment="Left" Height="22" Margin="78,292,0,0" VerticalAlignment="Top" Width="631" IsManipulationEnabled="True"/>
        <TextBlock x:Name="txtStatus" HorizontalAlignment="Left" Margin="274,362,0,0" TextWrapping="Wrap" Text=" S T A T U S" VerticalAlignment="Top" RenderTransformOrigin="-1.571,3.592" Height="21" Width="200" TextAlignment="Center" Foreground="Black" Background="#FFFF1200" />
        <Button x:Name="btnShortCut" Content="Set Autostart Shortcut" Click="ButtonCreateShortcut_Click" Height="40" Width="125"/>
    </Grid>
</Window>
